{
  "name": "@ahkui/oh-my-gitlab",
  "version": "0.0.0",
  "description": "Manage multiple repository in Gitlab",
  "bin": {
    "oh-my-gitlab": "index.js"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "lint": "eslint . --ext .js",
    "postversion": "git push --follow-tags"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/ahkui/oh-my-gitlab.git"
  },
  "keywords": [
    "gitlab"
  ],
  "license": "MIT",
  "dependencies": {
    "gitlab": "latest",
    "colors": "latest",
    "lodash": "latest",
    "yargs": "latest"
  },
  "devDependencies": {
    "eslint": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-node": "latest",
    "husky": "latest",
    "lint-staged": "latest"
  }
}
